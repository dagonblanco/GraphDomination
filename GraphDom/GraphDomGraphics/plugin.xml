<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            name="GraphDomGraphics Diagram Type"
            type="GraphDomGraphics"
            id="GraphDomGraphics.GraphDomGraphicsDiagramType">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            name="GraphDomGraphics Diagram"
            class="graphdomgraphics.diagram.GraphDomGraphicsDiagramTypeProvider"
            id="GraphDomGraphics.GraphDomGraphicsDiagramTypeProvider">
         <diagramType
               id="GraphDomGraphics.GraphDomGraphicsDiagramType">
         </diagramType>
         <imageProvider
               id="GraphdomImageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   
	<extension
		point = "org.eclipse.ui.newWizards">
		<wizard
        class="graphdomgraphics.ui.CreateGraphdomProjectWizard"
        finalPerspective="GraphDomPerspective"
        id="graphdomgraphics.ui.CreateSampleProjectWizard"
        name="%wizard.name"
        project="true">
			<description>%wizard.description</description>
			<selection class="org.eclipse.core.resources.IResource"/>
		</wizard>
		<wizard
        class="graphdomgraphics.ui.CreateDiagramWizard"
        finalPerspective="GraphDomPerspective"
        id="graphdomgraphics.ui.CreateDiagramWizard"
        name="%wizard.name.0">
     <description>
        %wizard.description.0
     </description>
     <selection
           class="org.eclipse.core.resources.IContainer">
     </selection>
		</wizard>
	</extension>
  
<extension
    point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
    <propertyContributor contributorId="GraphDomGraphics.PropertyContributor"> 
        <propertyCategory category="Graphiti"> 
        </propertyCategory> 
    </propertyContributor> 
</extension>
       
<extension
      point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
   <propertyTabs
          contributorId="GraphDomGraphics.PropertyContributor">
   <propertyTab
         category="Graphiti"
         id="graphiti.main.tab"
         label="%propertyTab.label">
    </propertyTab>
   </propertyTabs>
</extension>
     
<extension
      point="org.eclipse.ui.views.properties.tabbed.propertySections">
    <propertySections
          contributorId="GraphDomGraphics.PropertyContributor">
        <propertySection
              class="graphdomgraphics.properties.NodeSection"
              filter="graphdomgraphics.properties.NodeFilter"
              id="graphiti.main.tab.emfclass"
              tab="graphiti.main.tab">
        </propertySection>
    </propertySections>
</extension>
 
    <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="graphdomgraphics.ui.GFPerspective"
            fixed="false"
            icon="icons/rialto16.gif"
            id="GraphDomPerspective"
            name="GraphDom"/>
   </extension>
   
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="graphdomgraphics.common.GraphdomImageProvider"
            id="GraphdomImageProvider">
      </imageProvider>
   </extension>
   
   <extension
      point="org.eclipse.core.resources.natures"
      id="graphdomgraphics.graphdomnature"
      name="GraphDom Nature">
      <runtime>
         <run class="graphdomgraphics.common.GraphdomProjectNature">
         </run>
      </runtime>
</extension>
   <extension
         id="GraphDomGraphics"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="GraphDom">
         <property
               name="aboutText"
               value="GraphDom - Dominación en Grafos&#x0A;&#x0A;TFC&#x0A;&#x0A;Autor: David Gonzalo Blanco&#x0A;Tutor: Gregorio Hernández&#x0A;&#x0A;Facultad de Informática, Universidad Politécnica de Madrid">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="appName"
               value="GraphDom">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
</plugin>
